{
  "expo": {
    "name": "orby",
    "slug": "orby",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/orby_default_icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/orby_splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "versionCode": 1,
      "permissions": ["INTERNET"],
      "adaptiveIcon": {
        "foregroundImage": "./assets/ic_launcher_foreground.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.project.orby"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      "expo-secure-store",
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "e862a961-1e7d-4228-8e04-f9abcdd46efe"
      }
    }
  }
}
